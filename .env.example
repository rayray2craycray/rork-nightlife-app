# Rork Project Configuration
RORK_PROJECT_ID=your_project_id_here

# Toast POS Integration (for production)
# TOAST_CLIENT_ID=your_toast_client_id
# TOAST_CLIENT_SECRET=your_toast_client_secret
# TOAST_REDIRECT_URI=your_redirect_uri

